	<DataRequest xsi:type="LiquidCompartmentDataRequestData" Compartment="LeftUreter" Substance="Sodium"  Name="Mass"          Unit="ug"     Precision="0"/>
	<DataRequest xsi:type="LiquidCompartmentDataRequestData" Compartment="LeftUreter" Substance="Sodium"  Name="Molarity"      Unit="mmol/L" Precision="2"/>
