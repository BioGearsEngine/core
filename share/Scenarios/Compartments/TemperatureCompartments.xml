	<DataRequest xsi:type="ThermalCompartmentDataRequestData" Compartment="Ambient" Name="Temperature"         Unit="degC" Precision="3"/>
	<DataRequest xsi:type="ThermalCompartmentDataRequestData" Compartment="Ambient" Name="Heat"                Unit="J" Precision="3"/>	
  </DataRequests>	
  
	<Action xsi:type="AdvanceTimeData">
     <Time value="30" unit="s"/>       
  </Action>
</Scenario>