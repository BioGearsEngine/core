	</Action>
	<!--Return to normal environment-->
	<Action xsi:type="EnvironmentChangeData">
		<Conditions>
    <Name>LocalEnvironment</Name>
			<AmbientAerosol Name="Sarin">
				<Concentration value="0.0" unit="mg/m^3"/>
			</AmbientAerosol>
		</Conditions>
	</Action>
	<Action xsi:type="AdvanceTimeData">
