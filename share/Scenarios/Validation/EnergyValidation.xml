<?xml version="1.0" encoding="UTF-8"?>
<Scenario xmlns="uri:/mil/tatrc/physiology/datamodel" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" contentVersion="BioGears_6.3.0-beta" xsi:schemaLocation="">
    <Name>Energy Validation</Name>
    <Description>Validation of Energy data</Description>
    <InitialParameters><PatientFile>StandardMale.xml</PatientFile></InitialParameters>
	
  <DataRequests SamplesPerSecond="50">
    <DataRequest xsi:type="PhysiologyDataRequestData" Name="CoreTemperature" Unit="degC" Precision="1"/>
    <DataRequest xsi:type="PhysiologyDataRequestData" Name="CreatinineProductionRate" Unit="umol/s" Precision="3"/>
    <DataRequest xsi:type="PhysiologyDataRequestData" Name="KetoneProductionRate" Unit="umol/min" Precision="0"/>
    <DataRequest xsi:type="PhysiologyDataRequestData" Name="LactateProductionRate" Unit="mol/day" Precision="2"/>
    <DataRequest xsi:type="PhysiologyDataRequestData" Name="SkinTemperature" Unit="degC" Precision="1"/>	
    <DataRequest xsi:type="PhysiologyDataRequestData" Name="SweatRate" Unit="g/s" Precision="2"/>
    <DataRequest xsi:type="PhysiologyDataRequestData" Name="TotalMetabolicRate" Unit="kcal/day" Precision="0"/>
  </DataRequests>
	
    <Action xsi:type="AdvanceTimeData">
        <Time value="2" unit="min"/>	
    </Action>
</Scenario>

