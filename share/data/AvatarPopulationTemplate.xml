  <!-- Can we add a lower bound (16?) and upper bound (65?) -->
  <AgeDistribution group="YoungAdult" mean="23.5" diviation="3.75" unit="yr"/>
  <AgeDistribution group="Adult" mean="40.0" diviation="3.75" unit="yr"/>
  <AgeDistribution group="MiddleAge" mean="57" diviation="3.75" unit="yr"/> 
  
  <!-- May want to update data for height given it is now sub-distributions as opposed to random sampling -->
  <HeightDistribution  group="Male;Short"  mean="64" diviation="1.0" unit="in"/>
  <HeightDistribution  group="Male;Average"  mean="70" diviation="1.0" unit="in"/>
  <HeightDistribution  group="Male;Tall"  mean="76" diviation="1.0" unit="in"/>
  
  <HeightDistribution  group="Female;Short"  mean="61.5" diviation="1.0" unit="in"/>
  <HeightDistribution  group="Female;Average"  mean="65.5" diviation="1.0" unit="in"/>
  <HeightDistribution  group="Female;Tall"  mean="70.5" diviation="1.0" unit="in"/>
  
  <!-- Need to add a patient parameter for BMI which back calculates weight using BMI sample and height -->
  <BMIDistribution group="Underweight"   mean="18.5" diviation="1.0" unit="unitless"/>
  <BMIDistribution group="Healthyweight"   mean="21.5" diviation="1.0" unit="unitless"/>
  <BMIDistribution group="Overweight"   mean="25.0" diviation="1.0" unit="unitless"/>
  
