<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<PopulationGenerator xmlns="uri:/mil/tatrc/physiology/datamodel" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="uri:/mil/tatrc/physiology/datamodel xsd/BioGearsDataModel.xsd">
  <Populations>
    <Population name="RobotsWhoWalkOnThreeLegs" count="10">
      <Property>Robot</Property>
      <Property>Walker</Property>
      <Property>Tripod</Property>
    </Population>

    <Population name="BoatsWithTwoGuns"  count="10">
      <Property>Boats</Property>
      <Property>Dual</Property>
      <Property>Navy</Property>
    </Population>
  </Populations>

  <Sampling>
    <Heterogametic-SexDistribution>
      <BernoulliDistribution p="0.5" >Robot;Tripod</BernoulliDistribution>
      <WeightedDistribution name="Boats" >
        <Value weight="20">Male</Value>
        <Value weight="80">Female</Value>
      </WeightedDistribution>
    </Heterogametic-SexDistribution>

    <AgeDistribution>
      <NormalDistribution  mean="40.0" deviation="5.0" unit="yr">Navy</NormalDistribution>
    </AgeDistribution>

    <WeightDistribution>
      <NormalDistribution mean="75" deviation="10.0" unit="kg">Male</NormalDistribution>
      <NormalDistribution mean="60" deviation="6.0" unit="kg">Female</NormalDistribution>
    </WeightDistribution>

    <HeightDistribution>
      <NormalDistribution  mean="185" deviation="7.0" unit="cm">Male</NormalDistribution>
      <NormalDistribution  mean="170" deviation="7.0" unit="cm">Femle</NormalDistribution>
    </HeightDistribution>

    <BodyFatFractionDistribution>
      <NormalDistribution  mean="0.28" deviation="2.0"/>
    </BodyFatFractionDistribution>

    <MaxWorkRateDistribution>
      <NormalDistribution  mean="762.2" deviation="2.0" unit="W"/>
    </MaxWorkRateDistribution>

    <BloodTypeABODistribution>
      <WeightedDistribution name="Male">
        <Value weight="20">AB</Value>
        <Value weight="80">A</Value>
      </WeightedDistribution>
    </BloodTypeABODistribution>

    <BloodTypeRhDistribution>
      <BernoulliDistribution p="0.1">Boats</BernoulliDistribution>
    </BloodTypeRhDistribution>

    <AlveoliSurfaceAreaDistribution>
      <NormalDistribution mean="52.45" deviation="2.0" unit="m^2"/>
    </AlveoliSurfaceAreaDistribution>

    <HyperhidrosisDistribution>
      <NormalDistribution mean="0.0" deviation="2.0"/>
    </HyperhidrosisDistribution>

    <RightLungRatioDistribution>
      <NormalDistribution mean="0.525" deviation="2.0"/>
    </RightLungRatioDistribution>

    <SkinSurfaceAreaDistribution>
      <NormalDistribution mean="1.618"  deviation="2.0" unit="m^2"/>
    </SkinSurfaceAreaDistribution>

    <SleepAmountDistribution>
      <NormalDistribution   mean="8.0" deviation="2.0" unit="hr"/>
    </SleepAmountDistribution>

    <PainSusceptibilityDistribution>
      <NormalDistribution mean="0.0" deviation="2.0"/>
    </PainSusceptibilityDistribution>

    <BasalMetabolicRateDistribution>
      <NormalDistribution mean="1296.4" deviation="2.0" unit="kcal/day"/>
    </BasalMetabolicRateDistribution>

    <BloodVolumeBaselineDistribution>
      <NormalDistribution mean="4111.4" deviation="2.0" unit="mL"/>
    </BloodVolumeBaselineDistribution>

    <DiastolicArterialPressureBaselineDistribution>
      <NormalDistribution mean="73.5" deviation="2.0"  unit="mmHg"/>
    </DiastolicArterialPressureBaselineDistribution>

    <HeartRateBaselineDistribution>
      <NormalDistribution  mean="72.0" deviation="2.0" unit="1/min"/>
    </HeartRateBaselineDistribution>

    <RespirationRateBaselineDistribution>
      <NormalDistribution mean="16.0" deviation="2.0"  unit="1/min"/>
    </RespirationRateBaselineDistribution>

    <SystolicArterialPressureBaselineDistribution>
      <NormalDistribution  mean="114.0" deviation="2.0"  unit="mmHg" />
    </SystolicArterialPressureBaselineDistribution>

    <HeartRateMaximumDistribution>
      <NormalDistribution mean="180.0" deviation="2.0" unit="1/min" />
    </HeartRateMaximumDistribution>

    <HeartRateMinimumDistribution>
      <NormalDistribution  mean="0.001" deviation="0.0"  unit="1/min"/>
    </HeartRateMinimumDistribution>

    <FunctionalResidualCapacityDistribution>
      <NormalDistribution mean="1.7337" deviation="2.0"  unit="L" />
    </FunctionalResidualCapacityDistribution>

    <ResidualVolumeDistribution>
      <NormalDistribution mean="0.925"  deviation="2.0"  unit="L"/>
    </ResidualVolumeDistribution>

    <TotalLungCapacityDistribution>
      <NormalDistribution  mean="4.62" deviation="2.0" unit="L" />
    </TotalLungCapacityDistribution>
  </Sampling>
</PopulationGenerator>
