  <Sampling>
    <Heterogametic-SexDistribution>
      <BernoulliDistribution p="0.0">Male</BernoulliDistribution>
      <BernoulliDistribution p="1.0" >Female</BernoulliDistribution>
    </Heterogametic-SexDistribution>

    <AgeDistribution>
      <NormalDistribution strategy="resample" mean="23.5"  deviation="3.75"  
                          min="18" max="31" unit="yr">YoungAdult</NormalDistribution>
      <NormalDistribution strategy="resample" mean="40.0" deviation="3.75" 
                           min="32" max="47" unit="yr">Adult</NormalDistribution>
      <NormalDistribution strategy="resample" mean="57"  deviation="3.75"  
                           min="48" max="64" unit="yr">MiddleAge</NormalDistribution> 
      </AgeDistribution>

    <WeightDistribution>
    </WeightDistribution>

    <HeightDistribution>
        <NormalDistribution  mean="64" deviation="1.0" unit="in">Male;Short</NormalDistribution>
        <NormalDistribution  mean="70" deviation="1.0" unit="in">Male;Average</NormalDistribution>
        <NormalDistribution  mean="76" deviation="1.0" unit="in">Male;Tall</NormalDistribution>

        <NormalDistribution  mean="58.5" deviation="1.0" unit="in">Female;Short</NormalDistribution>
        <NormalDistribution  mean="65.5" deviation="1.0" unit="in">Female;Average</NormalDistribution>
        <NormalDistribution  mean="70.5" deviation="1.0" unit="in">Female;Tall</NormalDistribution>
    </HeightDistribution>

    <BodyMassIndexDistribution>
      <NormalDistribution strategy="clamp" mean="15.5" deviation="1.5" 
                          min="12" max="18.5"  unit="unitless">UnderWeight</NormalDistribution>
      <NormalDistribution strategy="clamp" mean="21.5" deviation="1.5" 
                                                       unit="unitless">HealthyWeight</NormalDistribution>
      <NormalDistribution strategy="clamp" mean="27.5" deviation="1.5"  
                          min="24.5" max="42"  unit="unitless">OverWeight</NormalDistribution>
    </BodyMassIndexDistribution>

    <BodyFatFractionDistribution>
    </BodyFatFractionDistribution>

    <MaxWorkRateDistribution>
    </MaxWorkRateDistribution>

    <BloodTypeABODistribution>
    <!-- Enum values like CDM::enumBloodType can be auto converted by value or string
         The generator isn't the best at handling rouge values, but it will show validation
         fails when trying to write out the file
    -->
    <DiscreteUniformDistribution a="0" b="3"/>
    <WeightedDistribution name="Male">
        <Value weight="40">A</Value>
        <Value weight="10">AB</Value>
        <Value weight="40">B</Value>
        <Value weight="20">O</Value>
      </WeightedDistribution>
      <SelectionCycle name="Female">
        <Value>O</Value>
        <Value>A</Value>
        <Value>B</Value>
        <Value>AB</Value>
      </SelectionCycle> 
    </BloodTypeABODistribution>

    <BloodTypeRhDistribution>
       <!--When self closed a tag will be the default if no other matches are found-->
       <BernoulliDistribution p="0.5"/>
    </BloodTypeRhDistribution>

    <AlveoliSurfaceAreaDistribution>
    </AlveoliSurfaceAreaDistribution>

    <HyperhidrosisDistribution>
    </HyperhidrosisDistribution>

    <RightLungRatioDistribution>
       <ContinuousUniformDistribution a="0.3" b="0.6" unit="unitless"/>
    </RightLungRatioDistribution>

    <SkinSurfaceAreaDistribution>

    </SkinSurfaceAreaDistribution>

    <SleepAmountDistribution>
    </SleepAmountDistribution>

    <PainSusceptibilityDistribution>
    </PainSusceptibilityDistribution>

    <BasalMetabolicRateDistribution>
    </BasalMetabolicRateDistribution>

    <BloodVolumeBaselineDistribution>
    </BloodVolumeBaselineDistribution>

    <DiastolicArterialPressureBaselineDistribution>
    </DiastolicArterialPressureBaselineDistribution>

    <HeartRateBaselineDistribution>
    </HeartRateBaselineDistribution>

    <RespirationRateBaselineDistribution>
    </RespirationRateBaselineDistribution>

    <SystolicArterialPressureBaselineDistribution>
    </SystolicArterialPressureBaselineDistribution>

    <HeartRateMaximumDistribution>
    </HeartRateMaximumDistribution>

    <HeartRateMinimumDistribution>
    </HeartRateMinimumDistribution>

    <FunctionalResidualCapacityDistribution>
    </FunctionalResidualCapacityDistribution>

    <ResidualVolumeDistribution>
    </ResidualVolumeDistribution>

